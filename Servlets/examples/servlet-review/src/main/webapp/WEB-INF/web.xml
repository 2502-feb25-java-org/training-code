<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>servlet-review</display-name>
  
  
    <servlet>
    <servlet-name>MyHttpServlet</servlet-name>
    <servlet-class>com.revature.servlets.MyHttpServlet</servlet-class>
	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyHttpServlet</servlet-name>
    <url-pattern>/http</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MyFirstServlet</servlet-name>
    <servlet-class>com.revature.servlets.MyGenericServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyFirstServlet</servlet-name>
    <url-pattern>/generic</url-pattern>
  </servlet-mapping>

</web-app>